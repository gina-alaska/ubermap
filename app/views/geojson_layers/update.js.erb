var map = $('#map').data('container').map;
var layer = $('[data-map="reload"]').data('layer');

layer.removeFrom(map);
layer = MapLayer.fromConfig(map, <%= leaflet_params(@geojson_layer).html_safe %>);
layer.addTo(map);

if ($('[data-map="reload"]').length > 0) {
  $('[data-map="reload"]').data('layer', layer)
}
